{
  "manifest_version": 2,
  "name": "Color Contrast Checker Extension",
  "description": "Verifica el contraste de los colores.",
  "icons": { "180": "ico.png" },
  "version": "0.0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/js/index.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "ico.png",
    "default_title": "Color Contrast Checker"
  }
}

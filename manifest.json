{
  "manifest_version": 2,
  "name": "Color Contrast Checker Extension",
  "description": "Verifica el contraste de los colores.",
  "icons": { "180": "ico.png" },
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://*.com/*"],
      "js": ["/js/index.js"]
    }
  ],
  "browser_action": {
    "default_popup": "index.html"
  }
}
